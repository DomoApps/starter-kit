{
  "name": "da-starter",
  "version": "0.0.4",
  "da-webpack": "0.0.4",
  "description": "A starter for writing Domo Apps in ES6",
  "scripts": {
    "start": "NODE_ENV=development node server.js",
    "test-ci": "NODE_ENV=test ./node_modules/karma/bin/karma start ./karma.conf.js --single-run --browsers PhantomJS,Firefox",
    "test": "NODE_ENV=test ./node_modules/karma/bin/karma start ./karma.conf.js --single-run",
    "tdd": "NODE_ENV=test ./node_modules/karma/bin/karma start ./karma.conf.js --browsers Chrome",
    "eslint": "node_modules/.bin/eslint src/**",
    "coveralls": "npm run test-ci && node node_modules/lcov-filter/index.js ./coverage/phantomjs/lcov.info spec | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "build": "rm -rf ./dist && NODE_ENV=production ./node_modules/.bin/webpack && cp manifest.json dist/",
    "preversion": "npm run eslint && npm test",
    "version": "npm run build && npm run changelog && git add -A CHANGELOG.md",
    "postversion": "git push && git push --tags",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -w -r 0"
  },
  "author": "AppTeam6 <Squad.AppTeam6@domo.com>",
  "repository": "https://git.empdev.domo.com/AppTeam6/da-webpack",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^6.1.2",
    "babel": "^6.3.13",
    "babel-core": "^6.3.17",
    "babel-loader": "^6.2.0",
    "babel-plugin-add-module-exports": "^0.1.1",
    "babel-plugin-closure-elimination": "0.0.2",
    "babel-plugin-transform-runtime": "^6.3.13",
    "babel-preset-es2015-loose": "^6.1.3",
    "babel-preset-stage-1": "^6.3.13",
    "babel-register": "^6.3.13",
    "babel-runtime": "^6.3.13",
    "cdnjs": "^0.3.2",
    "chai": "^3.4.1",
    "chalk": "^1.1.1",
    "coveralls": "^2.11.6",
    "css-loader": "^0.23.0",
    "cssnano": "^3.4.0",
    "cz-conventional-changelog": "^1.1.5",
    "da-plop": "git+ssh://git@git.empdev.domo.com:AppTeam6/da-plop.git",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^2.0.0",
    "eslint-friendly-formatter": "^1.2.2",
    "eslint-loader": "^1.1.1",
    "express": "^4.13.3",
    "fs-extra": "^0.26.2",
    "glob": "^6.0.1",
    "html-webpack-plugin": "^1.7.0",
    "install": "^0.4.1",
    "istanbul": "^0.4.1",
    "istanbul-instrumenter-loader": "^0.1.3",
    "karma": "^0.13.15",
    "karma-babel-preprocessor": "^6.0.1",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^0.2.2",
    "karma-coverage": "^0.5.3",
    "karma-firefox-launcher": "^0.1.7",
    "karma-mocha": "^0.2.1",
    "karma-phantomjs-launcher": "^0.2.1",
    "karma-safari-launcher": "^0.1.1",
    "karma-sinon": "^1.0.4",
    "karma-webpack": "^1.7.0",
    "lcov-filter": "0.1.1",
    "mocha": "^2.3.4",
    "ng-annotate-webpack-plugin": "^0.1.2",
    "npm": "^3.5.2",
    "phantomjs": "^1.9",
    "portfinder": "^0.4.0",
    "postcss-browser-reporter": "^0.4.0",
    "postcss-import": "^7.1.3",
    "postcss-loader": "^0.8.0",
    "postcss-reporter": "^1.3.0",
    "precss": "^1.3.0",
    "progress": "^1.1.8",
    "raw-loader": "^0.5.1",
    "request": "^2.67.0",
    "ryuu-client": "^2.5.0",
    "sinon": "^1.17.2",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.9",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-dev-server": "^1.14.0"
  },
  "dependencies": {
    "babel-polyfill": "^6.3.14"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
